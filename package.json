		"publint": "^0.2.7",
		"svelte": "^3.54.0",
		"svelte-check": "^3.0.1",
		"tslib": "^2.4.1",
		"typescript": "^4.9.3",
		"vite": "^4.0.0"
	},
	"peerDependencies": {
		"svelte": ">=3.0.0"
	},
	"type": "module"
